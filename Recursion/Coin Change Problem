#include <bits/stdc++.h>
using namespace std;
void solve(vector<int>& arr,int& target,int output,int steps,int& ans){
    if(output == target){
        ans = min(steps,ans);
        cout<<steps<<" ";
        return;
    }
    if(output > target){
        return;
    }
    steps++;
    for(int i = 0 ; i< arr.size() ; i++){
        solve(arr,target,output+arr[i],steps,ans);
    }

}

int main(){
    int minimum = INT32_MAX;
    vector<int> arr{1,2};
    int target = 5;
    int output = 0;
    int steps = 0;
    int ans = INT_MAX;;
    solve(arr,target,output,steps,ans);
    cout<<endl;
    cout<<ans<<endl;

    return 0;
} 
